{
  "name": "P0sX-Client",
  "version": "0.0.1",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "pack": "node -r babel-register pack.js",
    "pack-all": "npm run package -- --all",
    "lint": "node ./node_modules/.bin/eslint",
    "lint-jsx": "node ./node_modules/.bin/eslint app/**/**.jsx",
    "electron": "electron ./",
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config webpack.config.production.js --progress --profile --colors",
    "watch-renderer": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config webpack.config.production.js --progress --profile --colors --watch"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "dependencies": {
    "hammerjs": "2.0.6",
    "jquery": "2.2.3",
    "materialize-css": "0.97.6"
  },
  "devDependencies": {
    "axios": "0.9.1",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0 ",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "classnames": "2.2.3",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^0.37.6",
    "electron-settings": "^1.0.4",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "fs": "0.0.2",
    "json-loader": "0.5.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-materialize": "0.14.3",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-thunk": "2.0.1",
    "webpack": "^1.13.0"
  }
}
