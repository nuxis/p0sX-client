{
    "name": "p0sx-client",
    "productName": "p0sX",
    "version": "0.0.1",
    "description": "p0sX is a Point of Sales application",
    "main": "src/main.js",
    "scripts": {
        "start": "electron-forge start",
        "dist:win64": "electron-forge make --arch x64",
        "dist:win32": "electron-forge make --arch ia32",
        "dist:linux64": "electron-forge make --arch x64",
        "lint": "eslint --ext .js --ext .jsx src"
    },
    "keywords": [],
    "author": {
        "name": "Nuxis",
        "email": "kradalby@kradalby.no"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nuxis/p0sX-client.git"
    },
    "license": "MIT",
    "build": {
        "appId": "org.nuxis.p0sX"
    },
    "config": {
        "forge": {
            "electronPackagerConfig": {
                "asar": true,
                "icon": "./build/icon.ico",
                "ignore": [
                    ".idea",
                    ".gitignore",
                    ".eslintrc",
                    ".github",
                    ".git",
                    ".webpack",
                    ".vscode",
                    "^/out$",
                    ".drone.yml",
                    ".drone.yml.sig",
                    ".eslintrc",
                    ".appveyor.yml",
                    "jsconfig.json"
                ]
            },
            "electronWinstallerConfig": {
                "loadingGif": "./build/app.gif",
                "setupIcon": "./build/icon.ico"
            }
        }
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "electron-forge": "^5.2.4",
        "electron-prebuilt-compile": "^8.2.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-config-standard-jsx": "^10.0.0",
        "eslint-config-standard-react": "^11.0.1",
        "eslint-plugin-immutable": "^1.0.0",
        "eslint-plugin-import": "^2.24.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-standard": "^5.0.0"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "electron": "^13.2.1",
        "electron-compile": "^6.4.4",
        "electron-devtools-installer": "^3.2.0",
        "electron-react-devtools": "^0.5.3",
        "electron-settings": "^4.0.2",
        "electron-squirrel-startup": "^1.0.0",
        "escpos-print": "^2.0.0",
        "flexboxgrid": "^6.3.1",
        "font-awesome": "^4.7.0",
        "history": "^5.0.1",
        "material-ui": "^0.20.2",
        "moment": "^2.29.1",
        "printj": "^1.2.2",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-addons-css-transition-group": "^15.6.2",
        "react-dom": "^17.0.2",
        "react-notifications": "^1.7.2",
        "react-redux": "^7.2.4",
        "react-router": "^5.2.0",
        "react-router-redux": "^4.0.8",
        "react-tap-event-plugin": "^3.0.3",
        "react-transition-group": "^4.4.2",
        "redux": "^4.1.1",
        "redux-saga": "^1.1.3",
        "reselect": "^4.0.0",
        "roboto-fontface": "^0.10.0",
        "serialport": "^9.2.0"
    }
}
